    <script>
        function cookieOk() {
            let today = new Date();
            let date = today.getDate();
            localStorage.setItem('cookie-alert-dismissed', 1);
            localStorage.setItem('cookie-accepted-date', date);
        }
    </script>
    <script>
        (function ($) {
            //Code to check if 7 days have elapsed since user accepted cookie
            let today = new Date();
            let currentDate = today.getDate();
            if (currentDate - localStorage.getItem('cookie-accepted-date') <= 7) {
                $('.cookie-alert').alert('close')
            } else if (currentDate - localStorage.getItem('cookie-accepted-date') > 7) {
                $('.cookie-alert').show()
            }
        })(jQuery)
    </script>
